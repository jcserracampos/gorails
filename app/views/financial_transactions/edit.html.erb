
<%= provide :page_title, I18n.t('financial_transactions.edit.header') %>
<%= provide :breadcrumb do %>
    <li><%= link_to  I18n.t('activerecord.models.financial_transaction.other'), financial_transactions_path %></li>
    <li><%= link_to  @financial_transaction.try(:id), @financial_transaction %></li>
    <li><%= I18n.t('financial_transactions.edit.header', resource: @financial_transaction.try(:id) ) %></li>
<% end %>

<main id="main-container" style="min-height: 530px;">
  <div class="content bg-gray-lighter">
    <div class="row items-push">
      <div class="col-sm-7">
        <h1 class="page-heading">
          <%= I18n.t('financial_transactions.new.title') %>
        </h1>
      </div>
    </div>
  </div>
  <div class="content">
    <div class="block">
      <div class="block-content">
        <%= render 'form' %>
      </div>
    </div>
  </div>
</main>

